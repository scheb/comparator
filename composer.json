{
    "name": "scheb/comparator",
    "type": "library",
    "description": "Compares to values for equality",
    "keywords": ["comparator", "compare", "matching", "equality"],
    "homepage": "https://github.com/scheb/comparator",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Scheb",
            "email": "me@christianscheb.de"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Scheb\\Comparator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scheb\\Comparator\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
